{
  "globals": {
    "JSX": true
  },
  "env": {
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["next", "plugin:prettier/recommended"],
  "rules": {
    "semi": 1,
    "eol-last": 1,
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
