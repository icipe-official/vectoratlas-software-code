{
  "name": "malaria-vector-api",
  "version": "0.0.1",
  "description": "## Description",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:ci": "TZ=UTC jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TZ=UTC jest --config ./test/jest-e2e.json",
    "migrations:create": "typeorm-ts-node-esm \"migration:generate\" \"-d\" \"./src/db/datasource.ts\"",
    "migrations:runall": "typeorm-ts-node-esm \"migration:run\" \"-d\" \"./src/db/datasource.ts\"",
    "migrations:revert": "typeorm-ts-node-esm \"migration:revert\" \"-d\" \"./src/db/datasource.ts\"",
    "migrations:runallprod": "typeorm-ts-node-esm \"migration:run\" \"-d\" \"./dist/db/datasource.js\""
  },
  "dependencies": {
    "@azure/storage-blob": "^12.12.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^10.3.10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/typeorm": "^10.0.2",
    "@types/flat": "^5.0.2",
    "apollo-server-express": "^3.9.0",
    "convict": "^6.2.3",
    "csvtojson": "^2.0.10",
    "flat": "^5.0.2",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.3.0",
    "jwks-rsa": "^2.1.4",
    "njwt": "^1.2.0",
    "nodemailer": "^6.9.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts",
      ".entity.ts",
      ".model.ts",
      "datasource.ts",
      "main.ts",
      ".strategy.ts",
      "src/db/migrations/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "lines": 85
      }
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}
