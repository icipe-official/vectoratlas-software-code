{
  "options": {
    "paths": {
      "root": "",
      "mbtiles": ""
    },
    "domains": ["localhost:8080", "127.0.0.1:8080"],
    "formatQuality": {
      "jpeg": 80,
      "webp": 90
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": false,
    "serveAllStyles": false,
    "serveStaticMaps": true,
    "tileMargin": 0
  },
  "data": {
    "countries": {
      "mbtiles": "geojson/countries.mbtiles"
    },
    "land": {
      "mbtiles": "geojson/land.mbtiles"
    },
    "oceans": {
      "mbtiles": "geojson/oceans.mbtiles"
    },
    "rivers_lakes": {
      "mbtiles": "geojson/rivers_lakes.mbtiles"
    },
    "overlays": {
      "mbtiles": "overlays/an_gambiae_map.mbtiles"
    }
  }
}
